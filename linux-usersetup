mkdir ~/git
git config --global pull.rebase true
git config --global core.excludesfile '~/git/.gitignore'
cat < EOF > ~/git/.gitignore
*.pyc
/.settings
/.project
/.pydevproject
core.*
*~
charlie_test*
EOF


sudo xmodmap -e "keycode 20 = underscore minus underscore minus"
